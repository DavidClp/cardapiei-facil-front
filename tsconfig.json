{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "target": "ES6",
    "module": "commonjs",
    "strict": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react", // Adicionando esta linha,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    }
  },
  "include": ["src/**/*", "./stores/**/*",  "stores/**/*"],
  "exclude": ["node_modules"]
}
