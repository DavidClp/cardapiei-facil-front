{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "noEmitOnError": true,
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "declaration": true,
     "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },

}
